{
	"eslint.autoFixOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.tslint": true,
		"source.fixAll.eslint": true
	},
	"editor.formatOnSave": true,
	"cloudflaretunnel.tunnel.askForPort": false,
	"cloudflaretunnel.tunnel.defaultPort": 8001,
	"cloudflaretunnel.tunnel.hostname": "blog-develop.dhravya.dev",
	"cSpell.enableFiletypes": [
		"html",
		"jsx",
		"js",
		"ts",
		"tsx",
		"css",
		"scss",
		"less",
		"md",
		"mdx",
		"markdown",
		"json",
		"jsonc",
		"yml",
		"yaml",
		"py",
		"rs"
	],
	"editor.tabSize": 2,
	"emmet.includeLanguages": {
    "mdx": "html",
    "md": "html",
},
"[javascript]": {
    "editor.quickSuggestions": {
        "strings": true
    }
},
}
